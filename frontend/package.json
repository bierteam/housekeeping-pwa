{
  "name": "frontend",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "start": "npm run serve",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "axios": "0.21.1",
    "bootstrap": "5.0.2",
    "bootstrap-vue": "2.21.2",
    "core-js": "3.16.0",
    "havetheybeenpwned": "1.0.1",
    "register-service-worker": "1.7.2",
    "vue": "2.6.14",
    "vue-router": "3.5.2",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-plugin-pwa": "4.5.13",
    "@vue/cli-plugin-router": "4.5.13",
    "@vue/cli-plugin-unit-jest": "4.5.13",
    "@vue/cli-plugin-vuex": "4.5.13",
    "@vue/cli-service": "4.5.13",
    "@vue/eslint-config-standard": "6.1.0",
    "@vue/test-utils": "1.2.2",
    "babel-eslint": "10.1.0",
    "chromedriver": "91.0.1",
    "eslint": "7.29.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-vue": "7.15.0",
    "geckodriver": "2.0.1",
    "vue-loader-v16": "npm:vue-loader@16.4.0",
    "vue-template-compiler": "2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "./node_modules/@vue/cli-plugin-unit-jest/jest-preset",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*"
    ]
  },
  "bugs": {
    "url": "https://github.com/bierteam/housekeeping-pwa/issues"
  },
  "contibutors": [
    "Oscar Wieman (https://github.com/oscrx)",
    "Nino van der Laan (https://github.com/NvdLaan)"
  ],
  "gitHooks": {
    "pre-commit": "npm run lint"
  },
  "homepage": "https://github.com/bierteam/housekeeping-pwa/README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bierteam/housekeeping-pwa.git"
  }
}
